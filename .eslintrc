{
  "extends": "airbnb/base",
  "env": {
    "mocha": true,
  },
  "rules": {
    "strict": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-loop-func": 0,
    "arrow-body-style": 0,
    "new-cap": 0,
    "no-use-before-define": [1, "nofunc"],
    "object-shorthand": 0,
    "prefer-rest-params" : 0,
    "newline-per-chained-call": 0,
    "no-console": 0
  }
}
